<resources>


    <style name="AppTheme" parent="Theme.MaterialComponents.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/blueDefault</item>
        <item name="colorPrimaryDark">@color/blueDark</item>
        <item name="colorAccent">@color/blueLight</item>
        <item name="android:colorBackground">@color/blueBackground</item>
        <item name="colorControlHighlight">@color/blackTransparent</item>
        <item name="android:statusBarColor">@color/blueDark</item>
    </style>

    
    <style name="AppTheme.Launcher">
        <item name="android:windowBackground">@drawable/launcher_screen</item>
        <item name="colorPrimaryDark">@color/blueDefault</item>
    </style>


    <style name="ChipTheme" parent="Widget.MaterialComponents.Chip.Action">
        <item name="android:textSize">16sp</item>
        <item name="android:fontFamily">@font/sfproregular</item>
    </style>
</resources>
